generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  first_name String
  last_name String
}

model Registrant {
  id Int @id @default(autoincrement())
  first_name String
  last_name String
  company_email String @unique
  company_name String
  company_phone String
  checked_in Boolean @default(false)
  walk_in Boolean @default(false)
  created_at DateTime
  updated_at DateTime
}
